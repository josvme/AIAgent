        if (true === $input->hasParameterOption(['--silent'], true)) {
            $output->setVerbosity(OutputInterface::VERBOSITY_SILENT);
            $shellVerbosity = -2;
        } elseif (true === $input->hasParameterOption(['--quiet', '-q'], true)) {
            $output->setVerbosity(OutputInterface::VERBOSITY_QUIET);
            $shellVerbosity = -1;
        } else {
